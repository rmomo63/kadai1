<%= form_for([post, comment]) do |f| %>
  <div class="field">
    <%= f.label :commenter %>
    <%= f.text_area :commenter %>
  </div>

  <div class="field">
    <%= f.label :comment_content %>
    <%= f.text_area :comment_content %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
